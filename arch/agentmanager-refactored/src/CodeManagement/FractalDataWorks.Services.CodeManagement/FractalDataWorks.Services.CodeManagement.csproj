<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.1</TargetFramework>
		<LangVersion>12.0</LangVersion>
		<Nullable>enable</Nullable>
		<TreatWarningsAsErrors>true</TreatWarningsAsErrors>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<RootNamespace>FractalDataWorks.Services.CodeManagement</RootNamespace>
		<AssemblyName>FractalDataWorks.Services.CodeManagement</AssemblyName>
		<PackageId>FractalDataWorks.Services.CodeManagement</PackageId>
		<Description>Abstract code management services for analysis, refactoring, and generation</Description>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="FractalDataWorks.Net" Version="1.0.0" />
		<PackageReference Include="FractalDataWorks.Services" Version="1.0.0" />
		<PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="8.0.0" />
		<PackageReference Include="System.Collections.Immutable" Version="8.0.0" />
	</ItemGroup>

</Project>